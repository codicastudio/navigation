{
    "name": "codicastudio/navigation",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/navigation",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Navigation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Navigation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Navigation\\NavigationServiceProvider"
            ],
            "aliases": {
                "Navigation": "Spatie\\Navigation\\Facades\\Navigation"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
